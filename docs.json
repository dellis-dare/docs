{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Zappl Documentation.",
  "colors": {
    "primary": "#117DF8",
    "light": "#117DF8",
    "dark": "#117DF8"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "About Zappl",
            "pages": [
              "about/introduction",
              "about/requirements",
              "about/security",
              "about/macOS-Components",
              "about/FAQ"
            ]
          },
          {
            "group": "Deploying Zappl",
            "pages": [
              "deploy/markdown",
              "deploy/code",
              "deploy/images",
              "deploy/settings",
              "deploy/navigation",
              "deploy/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "zappl.co",
          "href": "https://zappl.co",
          "icon": "location-pin"
        },
        {
          "anchor": "dare.tech",
          "href": "https://dare.tech",
          "icon": "location-pin"

        }
      ]
    }
  },
  "logo": {
    "light": "/logo/zappl_logo.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Zappl Support",
        "href": "mailto:helpdesk@dare.tech"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Zappl Portal",
      "href": "https://portal.zappl.co"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/dare-tech",
      "linkedin": "https://www.linkedin.com/company/dare-technology-ltd"
    }
  }
}